{
  "name": "solana-kit-prettified",
  "version": "0.1.0",
  "description": "Prettified type wrappers for @solana/kit â€” flattened IDE tooltips without losing type safety",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./token-2022": {
      "types": "./dist/programs/token-2022.d.ts",
      "import": "./dist/programs/token-2022.mjs",
      "require": "./dist/programs/token-2022.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --typecheck"
  },
  "peerDependencies": {
    "@solana-program/memo": ">=0.11.0",
    "@solana-program/system": ">=0.11.0",
    "@solana-program/token": ">=0.10.0",
    "@solana-program/token-2022": "^0.9.0",
    "@solana/kit": ">=6.0.0",
    "@solana/kit-plugins": ">=0.3.0"
  },
  "peerDependenciesMeta": {
    "@solana/kit-plugins": {
      "optional": true
    },
    "@solana-program/system": {
      "optional": true
    },
    "@solana-program/token": {
      "optional": true
    },
    "@solana-program/token-2022": {
      "optional": true
    },
    "@solana-program/memo": {
      "optional": true
    }
  },
  "devDependencies": {
    "@solana-program/memo": "^0.11.0",
    "@solana-program/system": "^0.11.0",
    "@solana-program/token": "^0.10.0",
    "@solana-program/token-2022": "^0.9.0",
    "@solana/kit": "^6.0.1",
    "@solana/kit-plugins": "^0.3.0",
    "@solana/sysvars": "^5.0",
    "expect-type": "^1.3.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amilzbot/solana-kit-prettified.git"
  },
  "keywords": [
    "solana",
    "kit",
    "typescript",
    "prettify",
    "types",
    "IDE",
    "tooltips"
  ]
}
